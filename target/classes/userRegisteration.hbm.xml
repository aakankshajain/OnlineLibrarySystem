<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
 
 <hibernate-mapping>
 	<class name="online.library.system.POJOClasses.UserRefisteration" table="userRegisteration">
 		
 		<composite-id>
 			<key-property name="studentId" column="studentId"> </key-property>
 			<key-property name="emailId" column="emailId"></key-property>
 		</composite-id>
 		
 		<property name="name" column="name"></property>
 		<property name="phoneNumber" column="phoneNumber"></property>
 		<property name="password" column="password"></property>
 	</class>
 </hibernate-mapping>
 
